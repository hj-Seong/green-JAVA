package chap06_ref;

public class Ref {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 참조형의 저장 값 확인하기
		// String으로 확인하기
		String name1 = "그린";
		String name2 = "그린";
		String name3 = "green";
		System.out.println(name1 == name2);
		System.out.println(name1 == name3);
	}

}
